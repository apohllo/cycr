(define domains-of (term)
  (clet (
         (result ())
         (base-assertions (with-just-mt #$BaseKB (gather-index term)))
        ) 
        (cdolist (pair *domains*) 
                          (pif(< 0 (with-mt-list (cdr pair)
                                        (length (set-difference (gather-index term)
                                                                base-assertions) ))) 
                              (csetq result (adjoin (car pair) result))
                              ()))
        (ret result)))
